[client]
default-character-set = utf8

# Import all .cnf files from configuration directory
[mysqld]
skip-host-cache
skip-name-resolve
lower_case_table_names  = 1
transaction-isolation   = REPEATABLE-READ
collation-server        = utf8mb4_general_ci
character-set-server    = utf8mb4
max_connections         = 512
